generator client {
  url      = env("DATABASE_URL")
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  email     String @unique
  username  String @unique
  password  String
}

enum Role {
  USER
  ADMIN
  EDITOR
}
